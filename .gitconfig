# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
	name = Solomon
	email = solomonbroadbentnz@gmail.com
[credential]
	helper = store
[color]
	ui = auto
[core]
	editor = vim
[alias]
	solomon-log = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all
